            "http://mirror.funami.tech/arch/$repo/os/$arch": true,
            "http://mirror.misakamikoto.network/archlinux/$repo/os/$arch": true,
            "http://mirror.premi.st/archlinux/$repo/os/$arch": true,
            "https://ftp.lanet.kr/pub/archlinux/$repo/os/$arch": true,
            "https://mirror.anigil.com/archlinux/$repo/os/$arch": true,
            "https://mirror.funami.tech/arch/$repo/os/$arch": true,
            "https://mirror.misakamikoto.network/archlinux/$repo/os/$arch": true,
            "https://mirror.premi.st/archlinux/$repo/os/$arch": true,
            "https://seoul.mirror.pkgbuild.com/$repo/os/$arch": true
        }
